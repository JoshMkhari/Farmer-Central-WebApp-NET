
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_EmployeeLayout.cshtml";

}
@model IEnumerable<ST1109348.Models.FarmerModel>
<!-- Recent Sales -->
<div class="col-12">
    <div class="card recent-sales overflow-auto">

        <div class="filter">
            <a class="icon" href="#" data-bs-toggle="dropdown"><i class="bi bi-three-dots"></i></a>
            <ul class="dropdown-menu dropdown-menu-end dropdown-menu-arrow">
                <li class="dropdown-header text-start">
                    <h6>Filter</h6>
                </li>

                <li><a class="dropdown-item" href="#">Today</a></li>
                <li><a class="dropdown-item" href="#">This Month</a></li>
                <li><a class="dropdown-item" href="#">This Year</a></li>
            </ul>
        </div>

        <div class="card-body">
            <h5 class="card-title">Farmers </h5>

            <table class="table table-borderless datatable">
                <thead>
                    <tr>
                        <th scope="col">Farmer Name</th>
                        @*<th scope="col">Module Credits</th>
                        <th scope="col">Class Hours</th>
                        <th scope="col">Self Study Hours</th>*@
                    </tr>
                </thead>
                <tbody>

                    @foreach (var mod in Model)
                    {
                        <tr>
                            <td>@mod.FarmerID</td>
                            <td>@mod.FarmerUserName</td>
                            @*<td>@mod.NumberOfCredits</td>
                            <td>@mod.ClassHoursPerWeek</td>
                            <td>@mod.SelfStudyHours</td>*@
                        </tr>
                    }
                </tbody>
            </table>

        </div>

    </div>
</div><!-- End Recent Sales -->
